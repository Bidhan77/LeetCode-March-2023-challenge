class Solution {
    public boolean isCompleteTree(TreeNode root) {
        Queue<TreeNode> queue = new LinkedList();
        queue.add(root);
        
        while (!queue.isEmpty()) {
            TreeNode node = queue.poll();
            
            if (node == null && queue.peek() != null) return false; 
            
            if (node != null)  {
                queue.add(node.left);
                queue.add(node.right);
            }
        }
        
        return true;
    }
}
